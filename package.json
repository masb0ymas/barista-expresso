{
  "name": "express-gateway-typescript",
  "version": "1.2.0",
  "description": "Api Gateway with Express TypeScript",
  "main": "src/bin/app.ts",
  "scripts": {
    "dev": "npm run start:babel",
    "start": "nodemon ./src/bin/server.ts",
    "start:babel": "nodemon --exec babel-node --extensions \".ts\" ./src/bin/server.ts",
    "lint": "npx eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "refresh:env-jwt": "node ./src/scripts/generateEnvJWT.ts",
    "clean": "rm -rf ./dist",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "before-build": "yarn && npm run lint && npm run type-check && npm run clean",
    "build:ts": "npm run before-build && tsc -p .",
    "build:babel": "npm run before-build && babel src --out-dir dist --extensions \".ts\",\".js\"",
    "serve:staging": "NODE_ENV=staging node ./dist/bin/server.js",
    "serve:production": "NODE_ENV=production node ./dist/bin/server.js",
    "deploy:staging": "npm run build:babel && pm2 reload your_app_pm2",
    "deploy:production": "npm run build:babel && pm2 reload your_app_pm2",
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "standard-version",
    "release:pre": "npm run release -- --prerelease",
    "release:patch": "npm run release -- --release-as patch",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "googleapis": "^66.0.0",
    "handlebars": "^4.7.6",
    "helmet": "^4.3.1",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.17",
    "nodemailer-mailgun-transport": "^2.0.2",
    "pug": "^3.0.0",
    "query-string": "^6.13.7",
    "redis": "^3.0.2",
    "swagger-jsdoc": "^6.0.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/helmet": "^4.0.0",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.167",
    "@types/morgan": "^1.9.2",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.20",
    "@types/nodemailer": "^6.4.0",
    "@types/nodemailer-mailgun-transport": "^1.4.2",
    "@types/redis": "^2.8.28",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.2.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "standard-version": "^9.1.0",
    "supertest": "^6.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "express-gateway-typescript"
  },
  "keywords": [
    "express"
  ],
  "author": "masb0ymas",
  "license": "MIT"
}
